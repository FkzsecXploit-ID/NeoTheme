def clr():
    if os.name == 'nt':
        _ = os.system("cls")
    else:
        _ = os.system("clear")

def get_terminal_width():
    return os.get_terminal_size().columns

def center_decorated_line(left, content, right, color_start="", color_end=""):
    terminal_width = get_terminal_width()
    content_length = len(content) + len(color_start) + len(color_end)
    line_length = len(left) + len(right) + content_length
    padding = max(0, (terminal_width - line_length) // 2)
    return " " * padding + f"{left}{color_start}{content}{color_end}{right}"

